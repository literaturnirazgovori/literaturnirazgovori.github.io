---
[
    {% assign scheduleditems=false %}
    {% for post in site.posts %}
    {% if post.schedule and post.hidden %}
    {% if scheduleditems%},{% endif%}
    {
        "page": "{{ site.baseurl }}{{ post.url }}",
        "schedule": "{{post.schedule}}"
    }
    {% assign scheduleditems=true %}
    {% endif %}
    {% endfor %}
]